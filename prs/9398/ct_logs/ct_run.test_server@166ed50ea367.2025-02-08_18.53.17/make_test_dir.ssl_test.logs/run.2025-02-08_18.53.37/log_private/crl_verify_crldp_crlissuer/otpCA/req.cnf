# Purpose: Configuration for requests (end users and CAs).
ROOTDIR	        = /buildroot/otp/lib/ssl/make_test_dir/ct_logs/ct_run.test_server@166ed50ea367.2025-02-08_18.53.17/make_test_dir.ssl_test.logs/run.2025-02-08_18.53.37/log_private/crl_verify_crldp_crlissuer

[req]
input_password	= secret
output_password	= secret
default_bits	= 2048
