#!/bin/sh
ROOTDIR=/buildroot/otp/lib/ssl/make_test_dir/ct_logs/ct_run.test_server@cf6e1ba772ab.2024-09-06_09.47.45/make_test_dir.ssl_test.logs/run.2024-09-06_09.48.04/log_private/major/install

if [ -z "$RELDIR" ]
then
   RELDIR=$ROOTDIR/releases
fi

START_ERL_DATA=${1:-$RELDIR/start_erl.data}

$ROOTDIR/bin/run_erl -daemon /tmp/ $ROOTDIR/log "exec $ROOTDIR/bin/start_erl $ROOTDIR $RELDIR $START_ERL_DATA -sname ct_release_test-upgrade -heart"
