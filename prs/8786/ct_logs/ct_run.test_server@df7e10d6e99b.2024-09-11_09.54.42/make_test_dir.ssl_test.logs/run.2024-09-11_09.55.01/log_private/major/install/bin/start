#!/bin/sh
ROOTDIR=/buildroot/otp/lib/ssl/make_test_dir/ct_logs/ct_run.test_server@df7e10d6e99b.2024-09-11_09.54.42/make_test_dir.ssl_test.logs/run.2024-09-11_09.55.01/log_private/major/install

if [ -z "$RELDIR" ]
then
